{
  "name": "tilla",
  "version": "1.0.0",
  "description": "Transforming objects with JavaScript made easy",
  "repository": "https://github.com/TillaTheHun0/tilla",
  "bugs": "https://github.com/TillaTheHun0/tilla/issues",
  "main": "./dist/index.js",
  "scripts": {
    "build:dev": "npm run build -- -s",
    "build": "node ./node_modules/.bin/babel src --out-dir dist",
    "bump": "npx standard-version",
    "docs": "npx esdoc",
    "prepare": "npm run build",
    "standard": "node ./node_modules/standard/bin/cmd.js \"**/*.js\"",
    "test": "npm run standard && nyc mocha --require @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "transformation",
    "transformer",
    "dto",
    "permissions"
  ],
  "author": "Tyler Hall",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bluebird": "^3.5.3",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^7.1.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "husky": "^2.0.0",
    "mocha": "^6.0.0",
    "nyc": "^14.0.0",
    "standard": "^12.0.1",
    "standard-version": "^5.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/docs/**"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  }
}
