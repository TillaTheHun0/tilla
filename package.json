{
  "name": "tilla",
  "version": "1.0.2",
  "description": "Transforming objects with JavaScript made easy",
  "repository": "https://github.com/TillaTheHun0/tilla",
  "bugs": "https://github.com/TillaTheHun0/tilla/issues",
  "main": "./dist/index.js",
  "scripts": {
    "build:dev": "npm run build -- -s",
    "build": "node ./node_modules/.bin/babel src --out-dir dist",
    "bump": "npx standard-version",
    "docs": "npx rimraf docs && npx jsdoc src -r -R ./README.md -d docs",
    "prepare": "npm run build",
    "lint": "npx standard \"**/*.js\"",
    "test": "npm run lint && nyc mocha --require @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "transformation",
    "transformer",
    "dto",
    "permissions"
  ],
  "author": "Tyler Hall",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "husky": "^3.0.9",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1",
    "standard-version": "^7.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/docs/**",
      "/dist/**"
    ]
  }
}
