{
  "name": "tilla",
  "version": "0.0.4",
  "description": "Transforming objects with JavaScript made easy",
  "repository": "https://github.com/TillaTheHun0/tilla",
  "bugs": "https://github.com/TillaTheHun0/tilla/issues",
  "main": "./dist/index.js",
  "scripts": {
    "build:dev": "npm run build -- -s",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "prepare": "npm run build",
    "standard": "node ./node_modules/standard/bin/cmd.js \"**/*.js\"",
    "test": "npm run standard && nyc mocha --require babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "transformation",
    "transformer",
    "dto",
    "permissions"
  ],
  "author": "Tyler Hall",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "pre-commit": "^1.2.2",
    "standard": "^11.0.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
